{
  "name": "my-react-router-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "wireit",
    "dev": "wireit",
    "start": "wireit",
    "typecheck": "wireit",
    "typegen": "wireit",
    "deploy": "wireit",
    "postinstall": "pnpm typegen"
  },
  "dependencies": {
    "@cloudflare/vite-plugin": "1.12.3",
    "@react-router/node": "7.8.2",
    "@react-router/serve": "7.8.2",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "isbot": "5.1.30",
    "lucide-react": "0.542.0",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-router": "7.8.2",
    "tailwind-merge": "3.3.1"
  },
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "0.2.3",
    "@playwright/mcp": "0.0.36",
    "@react-router/dev": "7.8.2",
    "@react-router/fs-routes": "7.8.2",
    "@tailwindcss/vite": "4.1.13",
    "@types/node": "24.3.1",
    "@types/react": "19.1.12",
    "@types/react-dom": "19.1.9",
    "autoprefixer": "10.4.21",
    "esbuild": "0.25.9",
    "postcss": "8.5.6",
    "prettier": "3.6.2",
    "prettier-plugin-tailwindcss": "0.6.14",
    "shadcn": "3.2.1",
    "tailwindcss": "4.1.13",
    "tw-animate-css": "1.3.8",
    "typescript": "5.9.2",
    "unplugin-auto-import": "20.1.0",
    "vite": "7.1.4",
    "vite-tsconfig-paths": "5.1.4",
    "wireit": "0.14.12",
    "workerd": "1.20250906.0",
    "wrangler": "4.34.0"
  },
  "wireit": {
    "build": {
      "command": "react-router build",
      "dependencies": [
        "typecheck"
      ],
      "files": [
        "app/**",
        "vite.config.ts",
        "react-router.config.ts"
      ],
      "output": [
        "build/**"
      ]
    },
    "deploy": {
      "command": "wrangler deploy",
      "dependencies": [
        "build"
      ]
    },
    "dev": {
      "command": "react-router dev",
      "files": [
        "app/**",
        "vite.config.ts"
      ],
      "service": true
    },
    "start": {
      "command": "wrangler dev",
      "service": true
    },
    "typecheck": {
      "command": "tsc --build",
      "dependencies": [
        "typegen"
      ],
      "files": [
        "tsconfig.json",
        "app/**/*.ts",
        "app/**/*.tsx",
        "worker-configuration.d.ts",
        ".react-router/**"
      ],
      "output": [
        "tsconfig.tsbuildinfo"
      ]
    },
    "typegen": {
      "command": "node scripts/update-date.js && wrangler types && react-router typegen",
      "files": [
        "app/**",
        "react-router.config.ts",
        ".env",
        "wrangler.json"
      ],
      "output": [
        "worker-configuration.d.ts",
        ".react-router/**"
      ]
    }
  },
  "packageManager": "pnpm@10.13.1+sha512.37ebf1a5c7a30d5fabe0c5df44ee8da4c965ca0c5af3dbab28c3a1681b70a256218d05c81c9c0dcf767ef6b8551eb5b960042b9ed4300c59242336377e01cfad",
  "pnpm": {
    "overrides": {
      "esbuild": "$esbuild",
      "@esbuild-plugins/node-globals-polyfill": "$@esbuild-plugins/node-globals-polyfill",
      "workerd": "$workerd"
    },
    "onlyBuiltDependencies": []
  }
}
