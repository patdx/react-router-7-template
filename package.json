{
  "name": "my-react-router-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "wireit",
    "dev": "wireit",
    "start": "wireit",
    "typecheck": "wireit",
    "typegen": "wireit",
    "deploy": "wireit",
    "postinstall": "pnpm typegen"
  },
  "dependencies": {
    "@cloudflare/vite-plugin": "1.13.18",
    "@react-router/node": "7.9.5",
    "@react-router/serve": "7.9.5",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "isbot": "5.1.31",
    "lucide-react": "0.552.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-router": "7.9.5",
    "tailwind-merge": "3.3.1"
  },
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "0.2.3",
    "@playwright/mcp": "0.0.45",
    "@react-router/dev": "7.9.5",
    "@react-router/fs-routes": "7.9.5",
    "@tailwindcss/vite": "4.1.16",
    "@types/node": "24.9.2",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "autoprefixer": "10.4.21",
    "esbuild": "0.25.11",
    "postcss": "8.5.6",
    "prettier": "3.6.2",
    "prettier-plugin-tailwindcss": "0.7.1",
    "shadcn": "3.5.0",
    "tailwindcss": "4.1.16",
    "tw-animate-css": "1.4.0",
    "typescript": "5.9.3",
    "unplugin-auto-import": "20.2.0",
    "vite": "7.1.12",
    "vite-tsconfig-paths": "5.1.4",
    "wireit": "0.14.12",
    "workerd": "1.20251101.0",
    "wrangler": "4.45.3"
  },
  "wireit": {
    "build": {
      "command": "react-router build",
      "dependencies": [
        "typecheck"
      ],
      "files": [
        "app/**",
        "vite.config.ts",
        "react-router.config.ts"
      ],
      "output": [
        "build/**"
      ]
    },
    "deploy": {
      "command": "wrangler deploy",
      "dependencies": [
        "build"
      ]
    },
    "dev": {
      "command": "react-router dev",
      "files": [
        "app/**",
        "vite.config.ts"
      ],
      "service": true
    },
    "start": {
      "command": "wrangler dev",
      "service": true
    },
    "typecheck": {
      "command": "tsc --build",
      "dependencies": [
        "typegen"
      ],
      "files": [
        "tsconfig.json",
        "app/**/*.ts",
        "app/**/*.tsx",
        "worker-configuration.d.ts",
        ".react-router/**"
      ],
      "output": [
        "tsconfig.tsbuildinfo"
      ]
    },
    "typegen": {
      "command": "node scripts/update-date.js && wrangler types && react-router typegen",
      "files": [
        "app/**",
        "react-router.config.ts",
        ".env",
        "wrangler.json"
      ],
      "output": [
        "worker-configuration.d.ts",
        ".react-router/**"
      ]
    }
  },
  "packageManager": "pnpm@10.20.0+sha512.cf9998222162dd85864d0a8102e7892e7ba4ceadebbf5a31f9c2fce48dfce317a9c53b9f6464d1ef9042cba2e02ae02a9f7c143a2b438cd93c91840f0192b9dd",
  "pnpm": {
    "overrides": {
      "esbuild": "$esbuild",
      "@esbuild-plugins/node-globals-polyfill": "$@esbuild-plugins/node-globals-polyfill",
      "workerd": "$workerd"
    },
    "onlyBuiltDependencies": []
  }
}
