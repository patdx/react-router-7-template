{
  "files": [],
  "references": [
    { "path": "./tsconfig.node.json" },
    { "path": "./tsconfig.cloudflare.json" }
  ],
  "compilerOptions": {
    "incremental": true,
    "checkJs": true,
    "verbatimModuleSyntax": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "allowImportingTsExtensions": true,
    "paths": {
      // While we are using subpath imports, still needed here for
      // shadcn cli https://github.com/shadcn-ui/ui/issues/2960
      "#app/*": ["./app/*"]
    }
  }
}
